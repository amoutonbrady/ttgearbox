{
	"name": "tt-gearbox-website",
	"description": "Onepage website to present TT Gearbox company",
	"version": "1.0.0",
	"scripts": {
		"watch:tailwind": "postcss static/tailwind.css -o static/index.css -w",
		"build:tailwind": "postcss static/tailwind.css -o static/index.css",
		"dev": "yarn build:tailwind && sapper dev",
		"build": "cross-env NODE_ENV=production yarn build:tailwind && sapper build",
		"export": "cross-env NODE_ENV=production yarn build:tailwind && sapper export --legacy",
		"start": "node __sapper__/build",
		"format": "prettier --write --plugin-search-dir=. ./**/*.{html,svelte}"
	},
	"dependencies": {
		"@sendgrid/mail": "^7.0.0",
		"compression": "^1.7.4",
		"dotenv": "^8.2.0",
		"ky": "^0.19.0",
		"polka": "next",
		"scroll-behavior-polyfill": "^2.0.13",
		"sirv": "^0.4.2",
		"stripe": "^8.39.0",
		"svelte-i18n": "^3.0.3"
	},
	"devDependencies": {
		"@babel/core": "^7.9.0",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/plugin-transform-runtime": "^7.9.0",
		"@babel/preset-env": "^7.9.0",
		"@babel/runtime": "^7.9.2",
		"@fullhuman/postcss-purgecss": "^2.1.0",
		"@rollup/plugin-alias": "^3.0.1",
		"@rollup/plugin-commonjs": "^11.0.2",
		"@rollup/plugin-json": "^4.0.2",
		"@rollup/plugin-node-resolve": "^7.1.1",
		"@rollup/plugin-replace": "^2.3.1",
		"@types/stripe": "^7.13.22",
		"cross-env": "^7.0.2",
		"cssnano": "^4.1.10",
		"npm-run-all": "^4.1.5",
		"postcss-cli": "^7.1.0",
		"prettier": "^2.0.2",
		"prettier-plugin-svelte": "^0.7.0",
		"rollup": "^2.3.2",
		"rollup-plugin-babel": "^4.4.0",
		"rollup-plugin-svelte": "^5.2.1",
		"rollup-plugin-terser": "^5.3.0",
		"sapper": "^0.27.12",
		"svelte": "^3.20.1",
		"tailwindcss": "^1.2.0"
	}
}
